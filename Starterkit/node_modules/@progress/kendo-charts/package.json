{
  "name": "@progress/kendo-charts",
  "description": "Kendo UI platform-independent Charts library",
  "author": "Progress",
  "license": "SEE LICENSE IN license.txt",
  "version": "2.2.0",
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "es2015": "dist/es2015/main.js",
  "typings": "dist/npm/main.d.ts",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "sideEffects": true,
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider gulp start",
    "build-package": "cross-env NODE_OPTIONS=--openssl-legacy-provider gulp build-rollup-package build-cdn",
    "test": "cross-env TZ=Europe/Sofia jest",
    "watch-test": "jest --watch",
    "e2e": "cross-env NODE_OPTIONS=--openssl-legacy-provider TZ=Europe/Sofia ENV_BROWSER=Chrome_headless gulp e2e",
    "watch-e2e": "cross-env TZ=Europe/Sofia ENV_BROWSER=Chrome_headless gulp watch-e2e",
    "e2e:ci": "cross-env TZ=Europe/Sofia CHROME_BIN=`which chromium-browser` NO_SANDBOX=true ENV_BROWSER=Chrome_headless npm run e2e",
    "e2e_visual:ci": "./bin/run-e2e-visual.sh",
    "lint": "eslint . --quiet",
    "semantic-release": "semantic-release pre && semantic-prerelease publish && semantic-release post",
    "prepare": "husky install"
  },
  "keywords": [
    "Kendo UI",
    "Progress",
    "DataViz",
    "Angular 2",
    "Charts"
  ],
  "peerDependencies": {
    "@progress/kendo-drawing": "^1.19.0"
  },
  "devDependencies": {
    "@progress/kendo-drawing": "^1.19.0",
    "@progress/kendo-e2e": "^0.6.0",
    "@progress/kendo-intl": "^3.1.1",
    "@progress/kendo-package-tasks": "^4.0.44",
    "@progress/kendo-svg-icons": "^1.5.0",
    "@progress/kendo-theme-default": "^6.3.0",
    "@progress/pako-esm": "^1.0.1",
    "@progress/semantic-prerelease": "^3.0.2",
    "@types/jest": "^26.0.24",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^1.1.5",
    "d3-scale-chromatic": "^3.0.0",
    "es-jest": "1.2.0",
    "eslint": "^8.10.0",
    "ghooks": "^1.0.3",
    "gulp": "^4.0.0",
    "husky": "^7.0.1",
    "jest-cli": "26.6.3",
    "semantic-release": "^6.3.6",
    "validate-commit-msg": "^1.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-charts.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@progress/semantic-prerelease/analyzeCommits",
    "generateNotes": "@progress/semantic-prerelease/generateNotes",
    "getLastRelease": "@progress/semantic-prerelease/getLastRelease",
    "verifyConditions": "@progress/semantic-prerelease/verifyConditions",
    "verifyRelease": "@progress/semantic-prerelease/verifyRelease"
  },
  "files": [
    "dist"
  ]
}